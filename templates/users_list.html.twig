{# templates/auth/users_list.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Users List{% endblock %}

{% block body %}
    <h1>Users List</h1>
    <ul>
        {% for user in users %}
            <li>{{ user.username }} - {{ user.email }}</li>
        {% endfor %}
    </ul>
{% endblock %}
